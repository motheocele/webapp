# Service Bus (Managed Identity)
SERVICEBUS_NAMESPACE=mot-dev-sbus-01.servicebus.windows.net
SERVICEBUS_QUEUE_NAME=mot-requests

# Web PubSub
WEBPUBSUB_HUB=mot
# Prefer AAD auth:
WEBPUBSUB_ENDPOINT=https://mot.webpubsub.azure.com
# Or use connection string:
# WEBPUBSUB_CONNECTION_STRING=Endpoint=...;AccessKey=...;Version=1.0;

# OpenClaw Gateway (required for real Mot responses)
OPENCLAW_GATEWAY_URL=http://127.0.0.1:18789
OPENCLAW_GATEWAY_TOKEN=__SET_ME__
# Optional: override which agent handles requests
OPENCLAW_AGENT=openclaw:main
# Optional tuning
OPENCLAW_TIMEOUT_MS=90000
OPENCLAW_MAX_OUTPUT_TOKENS=700

# Worker tuning
MAX_CONCURRENT_CALLS=4
PREFETCH_COUNT=20
